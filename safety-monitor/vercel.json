{
    "framework": "vite",
    "installCommand": "rm -rf node_modules && npm install",
    "buildCommand": "npm run build",
    "rewrites": [
      {
        "source": "/api/:match*",
        "destination": "https://thingsboard.cloud/api/:match*"
      },
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ]
  }